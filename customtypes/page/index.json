{
  "id": "page",
  "label": "Page",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": false,
          "single": "heading1,heading2,heading3"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "title": {
              "type": "SharedSlice"
            },
            "button": {
              "type": "SharedSlice"
            },
            "announcement": {
              "type": "SharedSlice"
            },
            "all_videos": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            },
            "video": {
              "type": "SharedSlice"
            },
            "image_text": {
              "type": "SharedSlice"
            },
            "columns": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            },
            "memberlist": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
